        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="-6dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="TextView" />

</LinearLayout>
